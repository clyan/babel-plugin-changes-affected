{
  "editor.formatOnSave": true,
  "editor.tabSize": 2,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "editor.quickSuggestions": {
    "strings": true,
    "other": true,
    "comments": true
  },
  "eslint.validate": [
    "typescript",
    "javascript",
    "javascriptreact",
    "json",
    "jsonc",
    "json5"
  ],
  "typescript.tsdk": "node_modules\\typescript\\lib"
}